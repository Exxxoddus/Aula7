  
pacote  exe2

classe  SessaoDeTreinamento ( var  experiencia :  Int ) {
    divertido  treinarA ( jogador : JogadorDeFutebol ) {
        jogador.correr ()
        jogador.fazerGol ()
        jogador.correr ()
        println ( " \ n experiência inicial de $ {jogador.nome} : $ {jogador.experiencia} " )
        jogador.experiencia + = 5
        println ( " \ n experiência final de $ {jogador.nome} : $ {jogador.experiencia} " )
    }
}
